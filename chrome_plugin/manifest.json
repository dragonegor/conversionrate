{
  "manifest_version": 2,
  "name": "A/B testing",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["startTest.js"]
    }
  ],
  "browser_action": {
    "default_icon": "ab-testing.svg"
  },
  "background": {
    "scripts": ["background.js"]
  }
}